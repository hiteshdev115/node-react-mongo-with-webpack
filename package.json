  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode development",
    "start": "concurrently \"npm run build\" \"cd src/server && nodemon app.js\" \"npm run client\"",
    "client": "webpack-dev-server --mode development --mode development --devtool inline-source-map --hot",
    "server": "cd src/server nodemon app.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
